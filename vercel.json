{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "2048mb",
          "functions": {
            "api/**/*.js": {
              "memory": 1024
            }
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "CUSTOM_ENV_VARIABLE": "value"
    },
    "public": true
  }